{
  "scripts": {
    "build:json": "ts-node --transpile-only ./build-json.ts",
    "build:gdocs": "ts-node ./build-gdocs.ts",
    "build:graphs": "ts-node ./build-graphs.ts",
    "build:corgi": "ts-node ./build-corgi.ts",
    "build:webhosting": "ts-node ./build-webhosting.ts",
    "start": "npm run build:json && npm run build:corgi && npm run build:webhosting",
    "test": "npm start && npm run build:gdocs && npm run build:graphs",
    "coverage": "nyc npm run test",
    "postcoverage": "nyc report --reporter lcov --reporter html --report-dir ../coverage/"
  },
  "dependencies": {
    "@ts-graphviz/node": "^0.4.0",
    "@types/js-yaml": "^4.0.1",
    "@types/node": "^15.6.0",
    "js-yaml": "^4.1.0",
    "ts-graphviz": "^0.15.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "nyc": "^15.1.0"
  }
}
