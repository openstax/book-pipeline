{
    "name": "Local Dev",
    "build": {
      "dockerfile": "../Dockerfile",
      "context": "..",
      // "target": "dev-env",
    },
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace/richb-press,type=bind,consistency=default",
    "mounts": [],
    "workspaceFolder": "/workspace/richb-press",
    "extensions": [
      "MS-vsliveshare.vsliveshare-pack"
    ],
    "settings": {
      "files.trimTrailingWhitespace": true,
      "files.insertFinalNewline": true,
      "terminal.integrated.scrollback": 100000
    }
  }
